{
  "name": "sudokusolver",
  "version": "2.0.0",
  "description": "Website about Sudoku",
  "main": "index.js",
  "scripts": {
    "dev-server": "live-server .",
    "sass-dev": "sass --watch --update --style=expanded assets/scss/main.scss:assets/css/style.css",
    "sass-prod": "sass --no-source-map --style=compressed assets/scss/main.scss:assets/css/style.css",
    "start": "npm-run-all dev-server sass-dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1"
  },
  "author": "Raman Gupta",
  "license": "ISC"
}
